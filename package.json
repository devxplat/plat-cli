  },
  "contributors": [
    "epicvinny <visuzano@hotmail.com> ()"
  ],
  "dependencies": {
    "@folder/xdg": "^4.0.1",
    "@google-cloud/sql": "^0.21.0",
    "better-sqlite3": "^12.2.0",
    "@googleapis/sqladmin": "^31.1.0",
    "@inkjs/ui": "^2.0.0",
    "@inquirer/prompts": "^7.8.3",
    "ansi-colors": "^4.1.3",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "googleapis": "^156.0.0",
    "ink": "^6.2.0",
    "ora": "^8.2.0",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@testing-library/react": "^16.3.0",
    "ava": "latest",
    "codecov": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-xo": "latest",
    "eslint-plugin-ava": "latest",
    "eslint-plugin-compat": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unicorn": "latest",
    "fixpack": "latest",
    "husky": "^9.1.7",
    "ink-testing-library": "^4.0.0",
    "lint-staged": "latest",
    "np": "^10.0.7",
    "nyc": "latest",
    "pinst": "^3.0.0",
    "prettier": "latest",
    "remark-cli": "^12.0.1",
    "remark-preset-github": "^4.0.4",
    "rimraf": "^6.0.1",
    "sinon": "^21.0.0"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "homepage": "https://github.com/epicvinny/plat-cli",
  "keywords": [
    "cli",
    "cloudsql",
    "postgresql",
    "migration",
    "google-cloud",
    "gcp",
    "database",
    "devops",
    "platform-engineering",
    "cloud-migration",
    "postgres",
    "sql",
    "infrastructure",
    "automation"
  ],
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "bin": {
    "plat-cli": "./bin/plat-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicvinny/plat-cli"
  },
  "files": [
    "bin/",
    "src/",
    "index.js",
    "README.md",
    "LICENSE",
    "config/",
    "examples/"
  ],
  "scripts": {
    "start": "node bin/plat-cli",
    "dev": "node bin/plat-cli",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepublishOnly": "yarn quality:check",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=html",
    "lint": "yarn run lint:js && yarn run lint:md",
    "lint:js": "eslint .",
    "lint:md": "remark . -qfo",
    "lint:fix": "eslint . --fix",
    "pretest": "yarn run lint:js",
    "test": "cross-env NODE_ENV=test ava",
    "test:ci": "cross-env NODE_ENV=test nyc ava --verbose --fail-fast",
    "test:coverage": "cross-env NODE_ENV=test nyc ava",
    "test:watch": "cross-env NODE_ENV=test ava --watch",
    "prepare": "husky",
    "quality:check": "yarn lint && yarn test",
    "quality:fix": "yarn lint:fix && yarn test",
    "build:check": "yarn quality:check && npm pack --dry-run",
    "build:pack": "npm pack",
    "get:version": "node scripts/get-version.js",
    "local:install": "node scripts/local-install.js",
    "local:uninstall": "npm uninstall -g plat-cli",
    "local:test": "yarn local:install && plat-cli --help && plat-cli --version && yarn local:uninstall",
    "local:clean": "rimraf plat-cli-*.tgz",
    "release": "np",
    "release:patch": "np patch",
    "release:minor": "np minor",
    "release:major": "np major",
    "release:dry": "np --dry-run",
    "release:preview": "np --preview",
    "publish:check": "yarn build:check && echo 'âœ… Ready for publishing!'",
    "publish:manual": "yarn quality:check && npm publish"
  },
  "np": {
    "yarn": true,
    "cleanup": true,
    "tests": true,
    "preview": true,
    "branch": "main"
  }
}
