  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "bin": {
    "plat-cli": "./bin/plat-cli"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/epicvinny/plat-cli"
  },
  "scripts": {
    "start": "node bin/plat-cli",
    "dev": "node bin/plat-cli",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=html",
    "lint": "yarn run lint:js && yarn run lint:md",
    "lint:js": "eslint .",
    "lint:md": "remark . -qfo",
    "pretest": "yarn run lint:js",
    "test": "cross-env NODE_ENV=test ava",
    "test:ci": "cross-env NODE_ENV=test nyc ava --verbose --fail-fast",
    "test:coverage": "cross-env NODE_ENV=test nyc ava",
    "test:watch": "cross-env NODE_ENV=test ava --watch",
    "prepare": "husky"
  }
}
