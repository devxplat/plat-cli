{
  "extension": [
    ".js"
  ],
  "include": [
    "src/**/*.js",
    "index.js"
  ],
  "exclude": [
    "**/*.test.js",
    "**/*.spec.js",
    "test/**",
    "node_modules/**",
    "coverage/**",
    ".nyc_output/**"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "reporter": ["lcov", "html", "text", "json-summary"],
  "all": true,
  "cache": true,
  "babel": false,
  "skip-full": false
}
